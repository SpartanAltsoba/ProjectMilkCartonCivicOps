{
  "name": "decision-graph-ai-agent",
  "version": "1.0.0",
  "description": "AI agent for generating child welfare decision chain diagrams",
  "main": "runDecisionGraphAgent.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/runDecisionGraphAgent.js",
    "dev": "ts-node-dev --respawn --transpile-only runDecisionGraphAgent.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.ts\"",
    "clean": "rimraf dist",
    "prepare": "npm run clean && npm run build"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "plantuml-encoder": "^1.4.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "child-welfare",
    "decision-chain",
    "ai-agent",
    "plantuml",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "**/__tests__/**/*.ts",
      "**/?(*.)+(spec|test).ts"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  }
}
