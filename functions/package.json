{
  "name": "civic-trace-ops-functions",
  "version": "1.0.0",
  "description": "Firebase Functions for CIVIC TRACE OPS",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "serve": "npm run build && firebase emulators:start --only functions",
